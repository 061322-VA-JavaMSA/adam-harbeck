<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://JAVA.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">


	<servlet>
		<servlet-name>employee-servlet</servlet-name>
		<servlet-class>com.revature.web.EmployeeServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>employee-servlet</servlet-name>
		<url-pattern>/employees</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>auth-servlet</servlet-name>
		<servlet-class>com.revature.web.AuthServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>auth-servlet</servlet-name>
		<url-pattern>/auth</url-pattern>
	</servlet-mapping>
	
		<servlet>
		<servlet-name>ticket-servlet</servlet-name>
		<servlet-class>com.revature.web.TicketServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ticket-servlet</servlet-name>
		<url-pattern>/tickets</url-pattern>
	</servlet-mapping>

</web-app>